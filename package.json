{
	"name": "svelte-demo-application",
	"version": "0.1.0",
	"private": true,
	"description": "Description for SvelteDemoApplication",
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"build": "svelte-kit build",
		"cleanup": "rimraf target/classes/static/",
		"cy:open": "cypress open",
		"e2e": "npm run e2e:ci -- --headed",
		"e2e:ci": "cypress run --browser chrome",
		"format": "prettier --write \"{,src/**/}*.{md,json,js,svelte,css,html,yml}\"",
		"lint": "eslint .",
		"lint:fix": "npm run lint --fix",
		"prepare": "husky install",
		"preview": "svelte-kit preview",
		"start": "svelte-kit dev --port 9000 --open true",
		"test": "jest src/main/webapp/",
		"test:watch": "npm run test -- --watch",
		"webapp:build": "npm run build",
		"webapp:prod": "npm run build",
		"webapp:test": "npm run test"
	},
	"dependencies": {
		"date-fns": "2.25.0"
	},
	"devDependencies": {
		"@fortawesome/free-solid-svg-icons": "5.15.4",
		"@sveltejs/adapter-static": "1.0.0-next.21",
		"@sveltejs/kit": "1.0.0-next.184",
		"@testing-library/jest-dom": "5.14.1",
		"@testing-library/svelte": "3.0.3",
		"autoprefixer": "10.3.7",
		"cypress": "8.6.0",
		"esbuild": "0.13.6",
		"esbuild-jest": "0.5.0",
		"eslint": "7.32.0",
		"eslint-plugin-cypress": "2.12.1",
		"eslint-plugin-jest-dom": "3.9.2",
		"eslint-plugin-svelte3": "3.2.1",
		"eslint-plugin-testing-library": "4.12.4",
		"fa-svelte": "3.1.0",
		"generator-jhipster-svelte": "0.6.0",
		"husky": "7.0.2",
		"jest": "26.6.3",
		"jest-junit": "13.0.0",
		"jest-sonar": "0.2.12",
		"jest-svelte-resolver": "1.0.0",
		"postcss": "8.3.9",
		"prettier": "2.4.1",
		"prettier-plugin-svelte": "2.4.0",
		"pretty-quick": "3.1.1",
		"rimraf": "3.0.2",
		"svelte": "3.43.2",
		"svelte-jester": "1.8.2",
		"tailwindcss": "2.2.17"
	}
