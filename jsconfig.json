{
	"extends": "./target/svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": "."
	},
	"include": ["src/main/webapp/app/**/*.js", "src/main/webapp/app/**/*.svelte"]
}
